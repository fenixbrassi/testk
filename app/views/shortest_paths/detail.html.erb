        <tr>
          <th>Name</th>
          <th>User Agent</th>
          <th>Opertin System</th>
          <th>Amount Visits</th>
        </tr>
      </thead>
      <tbody>
        <% @shortest_path.visits.each do |detail| %>
          <tr>
            <td><%= detail.ip_conn %></td>
            <td><%= detail.user_agent %></td>
            <td><%= detail.operating_system %></td>
            <td><%= detail.amount_visits %></td>
          </tr>
        <% end %>
      </tbody>
    </table>


</p>


