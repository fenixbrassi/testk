<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @shortest_path.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @shortest_path.description %>
</p>

<p>
  <strong>Shortestpath:</strong>
  <%= @shortest_path.shortestpath %>
</p>

